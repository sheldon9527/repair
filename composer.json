{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.1.*",
    "guzzlehttp/guzzle": "~6.0",
    "baum/baum": "~1.1",
    "socialiteproviders/qq": "^2.0",
    "socialiteproviders/weibo": "^2.0",
    "socialiteproviders/weixin-web": "^2.0",
    "tmd/auto-git-pull": "^1.1",
    "maatwebsite/excel": "~2.0.0",
    "doctrine/dbal": "^2.5",
    "intervention/image": "^2.3",
    "nesbot/carbon": "^1.20",
    "potsky/laravel-localization-helpers": "^1.3",
    "predis/predis": "^1.0",
    "dingo/api": "1.0.x@dev",
    "tymon/jwt-auth": "0.5.*",
    "lokielse/laravel-pinyin": "^1.1",
    "lucadegasperi/oauth2-server-laravel": "5.0.*",
    "league/flysystem-aws-s3-v3": "~1.0",
    "lokielse/omnipay-alipay": "dev-master",
    "ignited/laravel-omnipay": "2.*",
    "omnipay/paypal": "~2.0",
    "guzzlehttp/oauth-subscriber": "^0.3.0",
    "abraham/twitteroauth": "^0.6.4",
    "socialiteproviders/weixin": "^2.1",
    "socialiteproviders/manager": "2.1.2",
    "tecnickcom/tcpdf": "^6.2",
    "geohash/geohash": "1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
    },
   "repositories": {
      "packagist": {
          "type": "composer",
          "url": "https://packagist.phpcomposer.com"
      }
  }
}
